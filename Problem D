#include <stdio.h>
#include <string.h>
int main() 
{
    int t,i,n;
    scanf("%d", &t);
   char s[30];
    for (i = 0; i < t; i++) 
    {  scanf("%d", &n);
        getchar();
        fgets(s, sizeof(s), stdin);
        if(n!=5)
        { printf ("NO\n");}
        else if (strchr(s, 'T') && strchr(s, 'i') && strchr(s, 'm') && strchr(s, 'u') && strchr(s, 'r'))
        {printf("YES\n");} 
        else 
        {printf("NO\n");}
    }
}
