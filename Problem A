#include <stdio.h>
int main() 
{
    int a, b, c, i, t;
    scanf("%d", &t);

    for (i = 0; i < t; i++) 
    { scanf("%d %d %d", &a, &b, &c);
        if (a == b) 
        {printf("\n%d", c);} 
        else if (a == c) 
        {printf("\n%d", b);} 
        else 
        {printf("\n%d", a);}
    }
}
