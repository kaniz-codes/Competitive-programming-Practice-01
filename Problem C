#include <stdio.h>
int main() 
{
    int t,i;
    scanf("%d", &t);
    getchar();
   char s[30];
    for (i = 0; i < t; i++) 
    {   fgets(s, sizeof(s), stdin);
        if ((s[0]=='Y'|| s[0]=='y')&& (s[1]=='E'|| s[1]=='e')&& (s[2]=='s'|| s[2]=='S')) 
        {printf("YES\n");} 
        else 
        {printf("NO\n");}
    }
}
